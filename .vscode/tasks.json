{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build Module",
			"type": "shell",
			"command": "npm run build",
			"isBackground": false,
			"problemMatcher": [
				"$tsc"
			],
			"group": "build"
		},
		{
			"label": "Watch and Build",
			"type": "shell",
			"command": "npm run dev",
			"isBackground": true,
			"problemMatcher": [
				"$tsc-watch"
			],
			"group": "build"
		}
	]
}